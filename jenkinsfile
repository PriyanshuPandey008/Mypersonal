pipeline{
    agent any
    tools{
        nodejs 'nodejs'
    }
    stages{
        stage('checkout'){
            steps{
                git url:'https://github.com/PriyanshuPandey008/Mypersonal',branch: 'master'
            }
        }
        stage('install'){
            steps{
                bat 'npm install'
            }
        }
        stage('test'){
            steps{
                bat 'npm test'
            }
        }
        stage('built'){
            steps{
                bat 'npm run build'
            }
        }
    }
}